  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "type-check": "tsc -b",
    "android": "bun run build && bunx cap sync && cd android && ./gradlew assembleDebug",
    "cap:sync": "bunx cap sync",
    "cap:add:android": "bunx cap add android",
    "cap:add:ios": "bunx cap add ios",
    "cap:open:android": "bunx cap open android",
    "cap:open:ios": "bunx cap open ios",
    "cap:run:android": "bunx cap run android",
    "cap:run:ios": "bunx cap run ios",
    "cap:assets": "bunx @capacitor/assets generate --iconBackgroundColor '#7C3AED' --iconBackgroundColorDark '#7C3AED' --splashBackgroundColor '#7C3AED' --splashBackgroundColorDark '#7C3AED'"
  },
